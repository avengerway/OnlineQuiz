<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>quizbranchcontroller</servlet-name>
        <servlet-class>com.quiz.controller.quizbranchcontroller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>semestersubjectcontroller</servlet-name>
        <servlet-class>com.quiz.controller.semestersubjectcontroller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>subjectquestioncontroller</servlet-name>
        <servlet-class>com.quiz.controller.subjectquestioncontroller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>practiceresultcontroller</servlet-name>
        <servlet-class>com.quiz.controller.practiceresultcontroller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admincontroller</servlet-name>
        <servlet-class>com.quiz.controller.admincontroller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddQuestionController</servlet-name>
        <servlet-class>com.quiz.controller.AddQuestionController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signupcontroller</servlet-name>
        <servlet-class>com.quiz.controller.signupcontroller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logincontroller</servlet-name>
        <servlet-class>com.quiz.controller.logincontroller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getSubjectCount</servlet-name>
        <servlet-class>com.quiz.controller.getSubjectCount</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getMockTestController</servlet-name>
        <servlet-class>com.quiz.controller.getMockTestController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MockTestResultController</servlet-name>
        <servlet-class>com.quiz.controller.MockTestResultController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>quizbranchcontroller</servlet-name>
        <url-pattern>/quizbranchcontroller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>semestersubjectcontroller</servlet-name>
        <url-pattern>/semestersubjectcontroller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>subjectquestioncontroller</servlet-name>
        <url-pattern>/subjectquestioncontroller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>practiceresultcontroller</servlet-name>
        <url-pattern>/practiceresultcontroller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admincontroller</servlet-name>
        <url-pattern>/admincontroller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddQuestionController</servlet-name>
        <url-pattern>/AddQuestionController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signupcontroller</servlet-name>
        <url-pattern>/signupcontroller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logincontroller</servlet-name>
        <url-pattern>/logincontroller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getSubjectCount</servlet-name>
        <url-pattern>/getSubjectCount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getMockTestController</servlet-name>
        <url-pattern>/getMockTestController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MockTestResultController</servlet-name>
        <url-pattern>/MockTestResultController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <context-param>
        <param-name>Study</param-name>
        <param-value>STUDY</param-value>
    </context-param>
    <context-param>
        <param-name>Practice</param-name>
        <param-value>PRACTICE</param-value>
    </context-param>
    <context-param>
        <param-name>Mocktest</param-name>
        <param-value>MOCKTEST</param-value>
    </context-param>
    <context-param>
        <param-name>Onlinequiz</param-name>
        <param-value>ONLINEQUIZ</param-value>
    </context-param>
</web-app>
